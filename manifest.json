{
    "name": "Warehouse Property Tool",
    "version": "1.0",
    "description": "A simple tool for searching/excluding/including properties in the Data Warehouse.",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage",
        "http://warehouse.activepipe.com/api/onboarding/data/exclude"
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "16.png",
            "32": "32.png",
            "48": "48.png",
            "128": "128.png"
        }
    },
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://warehouse.activepipe.com/api/onboarding/data/exclude"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ]
        }
    ]
}